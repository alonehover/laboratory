<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0,maximum-scale=1.0, user-scalable=no">
    <title >Play</title>
    <link rel="stylesheet" href="css/play.css">
    <script src="js/play.js"></script>
</head>

<body class="runner">
    <div><img src="img/play/title.jpg" alt=""></div>
    <div class="play-game">
        <div id="tff-wrapper" class="tff-wrapper">
            <img  class="biaoti" src="img/play/biaoti.png" alt="">
            <div style="display:none">
                <img id="resources-1x" src="img/play/default_100_percent/100-sprite.png">
                <img id="resources-2x" src="img/play/default_200_percent/200-sprite.png">
            </div>
        </div>
        <div class="ctrl-box">
            <div class="btn-tips" id="btnTips">
                <img  src="img/play/tips.png" alt="">
            </div>

            <button id="jump" class="ctrl-jump ctrl-btn">Jump</button>        
        </div>
    </div>
</body>
</html>